<?xml version="1.0" encoding="UTF-8"?>
<config>
<!-- global模块参数 -->
<!--  拨号成功自动打开指定主页，配置为空则不打开 --> 
<homepage></homepage>
<!--  默认语言 --> 
<default_language>ru-ru</default_language>

<!-- WebUI参数-->

<!--  自动消失的对话框消失的时间 --> 
<dialogdisapear>3000</dialogdisapear>
<!--  tips自动消失的时间 --> 
<tip_disapear>3000</tip_disapear>
<!--  页面中自动刷新数据的时间 --> 
<update_interval>3000</update_interval> 
